{
  "name": "TodayAndTomorrow",
  "version": "1.0.0",
  "description": "Todo app to make better use of time",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/newOceanLiu/TodayAndTomorrow",
    "type": "git"
  },
  "author": "newOceanLiu",
  "license": "MIT",
  "scripts": {
    "start": "gulp"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "flow"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "import",
      "flowtype"
    ],
    "rules": {
      "no-console": 0,
      "react/jsx-uses-react": 1,
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-flowtype": "^1.0.0",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
